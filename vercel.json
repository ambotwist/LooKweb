{
    "version": 2,
    "buildCommand": "flutter pub get && flutter build web --release",
    "outputDirectory": "build/web",
    "framework": "flutter",
    "installCommand": "git clone https://github.com/flutter/flutter.git -b stable && ./flutter/bin/flutter config --enable-web",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}